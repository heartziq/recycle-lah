<!doctype html>
<html lang="en">
{{ template "header" }}
<body>
 <div class="container">
<h1>Test sending token</h1>
     <form method="post" >
      <br>
         <input type="submit" class="submit" >
       </form>
    <br />
<br />
{{ range $key, $value := . }}
  {{ if eq $key "Message" }}
    <p> {{ $value  }} <p>
  {{ end }}
  {{ if eq $key "Error" }}
     <p> {{  $value  }} <p>
  {{ end }}
{{ end }}
    </div>
 </body>
</html>