    <div class="ViewBookings">
        <table>
            <tr>
                <th>Id</th>
                <th>Lat</th>
                <th>Lng</th>
                <th>Address</th>
                <th>CreatedBy</th>
                <th>Collector</th>
                <th>Completed</th>
            </tr>

            {{ range $k, $v := .PickupList }}
            <tr>
                <td>
                    {{ $v.Id}}
                </td>
                <td>
                    {{ $v.Lat }}
                </td>
                <td>
                    {{ $v.Lng }}
                </td>
                <td>
                    {{ $v.Address }}
                </td>
                <td>
                    {{ $v.CreatedBy }}
                </td>
                <td>
                    {{ $v.Collector }}
                </td>
                <td>
                    Approved
                </td>
            </tr>
            {{ end }}
        </table>
    </div>
    <br />

