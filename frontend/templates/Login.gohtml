<!doctype html>
<html lang="en">

{{ template "header" .}}

<body>
    {{template "BodyTitle" .}}
    <br>
    <h3 class="Application">Please log in with user id and password!</h3>
    <div class="Application">
        <form method="post">
            <div style="padding-bottom: 19px;">
                <div class="InApplication">
                    <input class="InApplication" type="text" name="userid" placeholder="userid"><br>
                </div>
                <div class="InApplication">
                    <input class="InApplication" type="password" name="password" placeholder="Password"><br>
                </div>
            </div>
            <input class="Application" type="submit" value="Log In">
            <b><a href="/newuser">New User</a></b>
        </form>
    </div>
    <h4 class="Application" id="MsgToUser"></h4>
</body>

</html>